function getFedResMeetSeries(): integer;
begin
  var bar, count: integer;
  var token, line: string;
  var fedSeries: integer = CreateNamedSeries( 'FedResMeet' );
  line :=  '20070131,20070321,20070509,20070628,20070807,20070918,20071031,20071211'
        + ',20060131,20060328,20060510,20060629,20060808,20060920,20061025,20061212'
        + ',20050202,20050322,20050503,20050630,20050809,20050920,20051101,20051213'
        + ',20040128,20040316,20040504,20040630,20040810,20040921,20041110,20041214'
        + ',20030129,20030318,20030506,20030625,20030812,20030916,20031028,20031209'
        //+ ',20020130,20020319,20020507,20020626,20020813,20020924,20021106,20021210'
   //+ ',20010103,20010131,20010320,20010418,20010515,20010627,20010821,20010913,20010917,20011002,20011106,20011211' // Includes Unscheduled meeting also
        /// + ',20010131,20010320,20010515,20010627,20010821,20011002,20011106,20011211'
        //+ ',20000202,20000321,20000516,20000628,20000822,20001003,20001115,20001219'
        //+ ',19990203,19990330,19990518,19990630,19990824,19991005,19991116,19991221'
        //+ ',19980204,19980331,19980519,19980701,19980818,19980929,19981117,19981222'
        //+ ',19970205,19970325,19970520,19970802,19970819,19970930,19971112,19971216'
        //+ ',19960131,19960326,19960521,19960703,19960820,19960924,19961113,19961217'
        ;
  count := 0;
  token := GetToken( line, 0, ',' );
  while( token <> '' ) do
  begin
    bar := DateToBar( StrToInt( token ) );
    if( bar <> -1 ) then
    begin
      @fedSeries[bar] :=  300;
    end;
    count := count + 1;
    token := GetToken( line, count, ',' );
  end;
  Result := fedSeries;
end;
